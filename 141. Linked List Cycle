class Solution {
public:
    bool hasCycle(ListNode *head) {
        if(!head||!head->next)
            return false;
        
        ListNode *fast=head->next;
        ListNode *slow=head;
        while(fast&&fast->next)
        {
            if(fast==slow)
                return true;
            
            fast=fast->next->next;
            slow=slow->next;
        }
        return false;
    }
};
